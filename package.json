{
  "name": "@tknf/node-cgi",
  "description": "A program to run JavaScript cgi scripts on a web server such as Apache.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mast1ff/node-cgi"
  },
  "bugs": {
    "url": "https://github.com/mast1ff/node-cgi/issues"
  },
  "homepage": "https://github.com/mast1ff/node-cgi/tree/main/#readme",
