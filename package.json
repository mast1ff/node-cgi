  "keywords": [
    "node",
    "cgi",
    "script",
    "javascript",
    "ejs",
    "apache"
  ],
  "main": "./lib/node-cgi.js",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@tknf/eslint-config-typescript": "^1.0.8",
    "@tknf/prettier-config": "^1.0.8",
